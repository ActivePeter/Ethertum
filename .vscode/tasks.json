{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "native_debug_fastest build",
            "problemMatcher": [
                "$rustc"
            ],
            "type": "cargo",
            "command": "build",
            "args": [
                "--config=${workspaceFolder}/.cargo/config_build_fastest.toml",
                "--features=bevy/dynamic_linking"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
    ]
}