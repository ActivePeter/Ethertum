    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "native_debug_fastest",
            "program": "${workspaceFolder}/<executable file>",
            "preLaunchTask": "native_debug_fastest build",
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'ethertia'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=ethertia",
                    "--package=ethertia"
                ],
                "filter": {
                    "name": "ethertia",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'dedicated_server'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=dedicated_server",
                    "--package=ethertia"
                ],
                "filter": {
                    "name": "dedicated_server",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        }
    ]
}